///Reverse of number(palindrome)

int n,num,r,rev=0;
cout<<"Enter the number: ";
cin>>n;
num=n;
while(n!=0)
    {
    r=n%10;
    n=n/10;
    rev=rev*10+r;
    }
    cout<<"Value of n: "<<n<<endl;
    cout<<"Value of num: "<<num<<endl;
    cout<<rev<<endl;
    cout<<"reverse is: "<<rev<<endl;
    if(num==rev)
        cout<<"Number is palindrome.";
        else
         cout<<"Number is not palindrome";

/*
